h2.section-title НАШИ ФИЛИАЛЫ И ПАРТНЕРЫ
.partners#partners
  .partners__left
    .partners__block.partners__block--orange
      .partners__title Филиалы
      ul.partners__list
        li.partners__item.active(data-nameCity="spb") Санкт-Петербург
        li.partners__item(data-nameCity="rnd") Ростов-на-Дону
        li.partners__item(data-nameCity="nn") Нижний Новгород
    .partners__block.partners__block--blue
      .partners__title Партнеры
      ul.partners__list
        li.partners__item(data-nameCity="krn") Красноярск
        li.partners__item(data-nameCity="kln") Калининград
        li.partners__item(data-nameCity="nvr") Новоросийск
        li.partners__item(data-nameCity="var") Варна (Болгария)
        li.partners__item(data-nameCity="mur") Мурманск
        li.partners__item(data-nameCity="ast") Астрахань
        li.partners__item(data-nameCity="sta") Стамбул (Турция)
        li.partners__item(data-nameCity="arh") Архангельск
        li.partners__item(data-nameCity="vld") Владивосток
        li.partners__item(data-nameCity="rig") Рига (Латвия)
  .partners__right
    .partners__dots
      .partners__dot.partners__dot--1.partners__dot--orange.active(data-dotCity="spb")
        .partners__dot-text Санкт-Петербург
      .partners__dot.partners__dot--2.partners__dot--orange(data-dotCity="rnd")
        .partners__dot-text Ростов-на-Дону
      .partners__dot.partners__dot--3.partners__dot--orange(data-dotCity="nn")
        .partners__dot-text Нижний Новгород
      .partners__dot.partners__dot--4(data-dotCity="krn")
