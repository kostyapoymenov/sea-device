      .partners__dot.partners__dot--6(data-dotCity="nvr")
        .partners__dot-text Новоросийск
      .partners__dot.partners__dot--7(data-dotCity="var")
        .partners__dot-text Варна (Болгария)
      .partners__dot.partners__dot--8(data-dotCity="mur")
        .partners__dot-text Мурманск
      .partners__dot.partners__dot--9(data-dotCity="ast")
        .partners__dot-text Астрахань
      .partners__dot.partners__dot--10(data-dotCity="sta")
        .partners__dot-text Стамбул (Турция)
      .partners__dot.partners__dot--11(data-dotCity="arh")
        .partners__dot-text Архангельск
      .partners__dot.partners__dot--12(data-dotCity="vld")
